<?xml version="1.0"?>
<object name="Book"
        meta_type="Factory-based Type Information with dynamic views"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="plone">
   
 <property name="title" i18n:translate="">Book</property>
 <property name="description" i18n:translate=""></property>
 <property name="content_icon">Book.gif</property>
 <property name="content_meta_type">Book</property>
 <property name="product">Newspaper</property>
 <property name="factory">addBook</property>
 <property name="immediate_view">folder_listing</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
   <element value="Page"/>
   <element value="Wordage"/>
   <element value="Article"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="folder_summary_view"/>
  <element value="folder_tabular_view"/>
  <element value="atct_album_view"/>
  <element value="folder_listing"/>
  <element value="formshow"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="view" to="(selected layout)"/>
 <alias from="edit" to="base_edit"/>
 <alias from="form" to="formshow"/>
 <alias from="sharing" to="@@sharing"/>
 <action title="View" 
         action_id="view"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/base" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="New Holland Press" 
         action_id="newhollandpress"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/newhollandpress" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="Base" 
         action_id="base"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/base" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="PDF" 
         action_id="pdfshow"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/pdfshow" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="WEB" 
         action_id="web"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/web" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="FORM" 
         action_id="formshow"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/form" 
         visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" 
         action_id="edit"
         category="object" 
         condition_expr="not:object/@@plone_lock_info/is_locked_for_current_user"
         url_expr="string:${object_url}/edit" 
         visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
